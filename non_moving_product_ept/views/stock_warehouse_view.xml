<odoo>
    <record id="stock_warehouse_non_moving_report_form_view" model="ir.ui.view">
        <field name="name">stock.warehouse.non.moving.report.form.view</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="default_non_moving"/>
            </xpath>
        </field>
    </record>
</odoo>