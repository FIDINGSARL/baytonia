<odoo>
    <record id="sale_order_form_inherited_aramex" model="ir.ui.view">
        <field name="name">sale.order.form.inherited.aramex</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="aramex_return_send_shipping" type="object" string="Return Aramex Label"
                        class="oe_highlight"/>
                <button name="aramex_send_return_label_to_customer" type="object" string="Send Aramex Label"
                        class="oe_highlight"/>
            </xpath>
        </field>
    </record>
</odoo>