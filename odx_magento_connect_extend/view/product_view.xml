<odoo>
    <record model="ir.ui.view" id="product_product_magento_varient_form_view">
        <field name="name">product.template.magento.form.view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='categ_id']" position="after">
                <field name="compare_magento_varients"/>
            </xpath>
        </field>
    </record>
</odoo>

